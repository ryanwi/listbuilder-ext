{
  "manifest_version": 2,

  "name": "List Builder for Twitter",
  "description": "Build Twitter from any timeline view.",
  "version": "0.1",
  "permissions": [
  ],
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*.twitter.com/*"],
      "js": ["js/jquery-2.1.0.min.js", "js/main.js"]
    }
  ]
}
